pprint_structure <- function(x, prefix = "") {
if (is.data.frame(x)) {
cat(prefix, "data.frame with columns:\n", sep = "")
cat(prefix, "  ", paste(names(x), collapse = ", "), "\n\n", sep = "")
} else if (is.list(x)) {
for (n in names(x)) {
cat(prefix, "$", n, "\n", sep = "")
print_structure(x[[n]], paste0(prefix, "  "))
}
} else {
# skip non-dataframe and non-list entries
}
}
